{
  "kind": "singleType",
  "collectionName": "course_pages",
  "info": {
    "singularName": "course-page",
    "pluralName": "course-pages",
    "displayName": "CoursePage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "default": "Our Courses",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "pageInfo": {
      "type": "component",
      "repeatable": false,
      "component": "layout.page-info"
    },
    "excludedCourses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course"
    }
  }
}
